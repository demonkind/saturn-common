package com.huifu.saturn.common.constants.code.systemCode;

/**
 * 汇付天下域划分
 * 
 * @author zhanghaijie
 * @version $Id: ChinaPNRDomainEnum.java, v 0.1 2012-8-10 下午3:17:18 zhanghaijie Exp $
 */
public enum ChinaPNRDomainCode {

    ACCT_DOMAIN("ACCT_D", "账务域"),

    PAYMENT_DOMAIN("PAY_D", "支付域"),

    MEMBER_DOMAIN("MEM_D", "会员域"),

    ACQUIRER_DOMAIN("ACQ_D", "收单域"),

    PORTAL_DOMAIN("POR_D", "门户域"),

    INNER_DOMAIN("IN_D", "内部域");

    private ChinaPNRDomainCode(String domainCode, String domainName) {
        this.domainCode = domainCode;
        this.domainName = domainName;
    }

    /** 域代码 */
    private String domainCode;

    /** 域说明 */
    private String domainName;

    /**
     * 获取域码
     * 
     * @return
     */
    public String getDomainCode() {
        return domainCode;
    }

    /**
     * 获取域名
     * 
     * @return
     */
    public String getDomainName() {
        return domainName;
    }

}
