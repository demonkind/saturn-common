package com.huifu.saturn.common.lang.crypt;

/**
 * 
 * @author su.zhang
 * @version $Id: EncryptException.java, v 0.1 2012-9-6 下午02:44:21 su.zhang Exp $
 */
public class EncryptException extends Exception {
    


    /**
     * serialVersionUID
     */
    private static final long serialVersionUID = -7219675953108313089L;

    /**
     * 
     */
    public EncryptException() {
        super();
    }

    /**
     * @param message
     * @param cause
     */
    public EncryptException(String message, Throwable cause) {
        super(message, cause);
    }

    /**
     * @param message
     */
    public EncryptException(String message) {
        super(message);
    }

    /**
     * @param cause
     */
    public EncryptException(Throwable cause) {
        super(cause);
    }



}
